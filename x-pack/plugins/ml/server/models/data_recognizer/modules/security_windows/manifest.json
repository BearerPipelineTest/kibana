{
  "id": "security_windows",
  "title": "Security: Windows",
  "description": "Security: Windows. This module contains all shipping ML jobs for Windows host based threat hunting and detection. Any ECS (Elastic Common Schema) compatable Windows events can be used by this module.",
  "type": "windows data",
  "logoFile": "logo.json",
  "defaultIndexPattern": "winlogbeat-*,logs-*",
  "query": {
    "bool": {
      "must": [
        {
          "bool": {
            "should": [
              {
                "match": {
                  "host.os.family": {
                    "query": "windows",
                    "operator": "OR"
                  }
                }
              },
              {
                "match": {
                  "host.os.type": {
                    "query": "windows",
                    "operator": "OR"
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
